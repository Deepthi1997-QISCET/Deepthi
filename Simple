import java.util.*;
import java.lang.*;
import java.io.*;
class Simple
{
	public static void main (String[] args) throws java.lang.Exception
	{
	int a=5;
	if(a>0)
	{
		System.out.println("a is positive");
	}
	else if(a<0)
	{
		System.out.println("a is negative");
	}
	else
	{
		System.out.println("a is Zero");
	}
 
	}
}
